            RowBox[{"(", 
             RowBox[{"Pi", "/", "2"}], ")"}]}], "]"}]}], "}"}]}], "]"}]}], 
     ";", "\n", "\t\t", "\n", "\t\t", 
     RowBox[{"(*", 
      RowBox[{"The", " ", "animate", " ", "creates", " ", "the", " ", 
