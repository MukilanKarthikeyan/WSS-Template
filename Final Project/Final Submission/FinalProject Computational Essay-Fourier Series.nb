      RowBox[{"Max", "[", 
       RowBox[{"Abs", "@", "fourierCoeff"}], "]"}]}], ";", "\n", "\t\t", "\n",
      "\t\t", 
     RowBox[{"(*", 
      RowBox[{
      "The", " ", "following", " ", "grouping", " ", "of", " ", "code", " ", 
