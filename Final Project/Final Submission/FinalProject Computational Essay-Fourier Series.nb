    RowBox[{"(*", 
     RowBox[{
     "This", " ", "is", " ", "used", " ", "for", " ", "the", " ", "spacing", 
      " ", "of", " ", "the", " ", "rage", " ", "of", " ", "the", " ", 
      "graph"}], "*)"}], "\n", "\t\t\t", 
    RowBox[{
     RowBox[{"totalRadius", "=", 
      RowBox[{"Total", "[", 
       RowBox[{"Abs", "@", "fourierCoeff"}], "]"}]}], ";", "\n", "\t", "\n", 
     "\t\t", 
     RowBox[{"(*", 
      RowBox[{
      "Creates", " ", "a", " ", "list", " ", "of", " ", "the", " ", "circle", 
       " ", "fraphics", " ", "primitives", " ", "using", " ", "the", " ", 
       "radii", " ", "and", " ", "a", " ", "fixed", " ", "coordinates"}], "*)"}
      ], "\n", "\t\t\t", 
     RowBox[{"circleList", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Circle", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"-", "maxRadius"}], "-", "5"}], ",", "0"}], "}"}], ",", 
          "#1"}], "]"}], "&"}], "/@", 
       RowBox[{"Abs", "@", "fourierCoeff"}]}]}], ";", "\n", "\t\t\t", "\n", 
     "\t\t", 
     RowBox[{"(*", 
      RowBox[{
      "sets", " ", "up", " ", "the", " ", "coordinates", " ", "that", " ", 
       "used", " ", "to", " ", "center", " ", "and", " ", "move", " ", 
       "different", " ", "parts", " ", "of", " ", "the", " ", "visual"}], " ",
       "*)"}], "\n", "\t\t\t", 
     RowBox[{"basicCoord", "=", 
      RowBox[{"MapThread", "[", 
       RowBox[{"Times", ",", 
        RowBox[{"{", 
         RowBox[{"fourierCoeff", ",", 
          RowBox[{"Cos", "[", 
           RowBox[{
            RowBox[{"Range", "[", "numCir", "]"}], "*", "variable"}], "]"}]}],
          "}"}]}], "]"}]}], ";", "\n", "\t\t\t", 
     RowBox[{"manipulatedCoord", "=", 
      RowBox[{"Transpose", "[", 
       RowBox[{"{", 
