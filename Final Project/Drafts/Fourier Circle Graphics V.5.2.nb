Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "circleSmoothietheRest", "]"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{"circleSmoothietheRest", "[", 
   RowBox[{"function_", ",", "variable_", ",", "numCir_"}], "]"}], ":=", "\n", 
  RowBox[{"Block", "[", "\n", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"proto", "=", 
       RowBox[{
        RowBox[{"FourierSeries", "[", 
         RowBox[{"function", ",", "variable", ",", "numCir"}], "]"}], "//", 
        "ComplexExpand"}]}], ",", "protoList2", ",", "protoList3", ",", 
      "protoList4", ",", "cosCoeff", ",", "sinCoeff", ",", "\n", 
      "basicCosCoord", ",", "basicSinCoord", ",", "totalRadius", ",", 
      "manipulatedCosCoord", ",", "manipulatedSinCoord", ",", 
      "manipulatedCoord", ",", "lastYCoord", ",", "plotCoord", ",", 
      "cirCenter", ",", "cirArguments", ",", "lastLinetoYaxis", ",", 
      "combinedCir", ",", "lineSeries", ",", "range", ",", "fourierCoeff"}], 
     "}"}], ",", "\n", 
    RowBox[{
     RowBox[{"Print", "[", "proto", "]"}], ";", "\n", "\[IndentingNewLine]", 
     RowBox[{"protoList2", "=", 
      RowBox[{"Drop", "[", 
       RowBox[{
        RowBox[{"Apply", "[", 
         RowBox[{"List", ",", "proto"}], "]"}], ",", "1"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"protoList3", "=", 
      RowBox[{"Drop", "[", 
       RowBox[{"protoList2", ",", 
        RowBox[{"-", "numCir"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"protoList4", "=", 
      RowBox[{"Drop", "[", 
       RowBox[{"protoList2", ",", "numCir"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cosCoeff", "=", 
      RowBox[{"Insert", "[", 
       RowBox[{
        RowBox[{"DeleteCases", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"protoList3", "/.", 
            RowBox[{"Times", "\[Rule]", "List"}]}], "//", "Flatten"}], ",", 
          "_Cos"}], "]"}], ",", "1", ",", "2"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"sinCoeff", "=", 
      RowBox[{"DeleteCases", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"protoList4", "/.", 
          RowBox[{"Times", "\[Rule]", "List"}]}], "//", "Flatten"}], ",", 
        "_Sin"}], "]"}]}], ";", "\n", "\n", 
     RowBox[{"fourierCoeff", "=", 
      RowBox[{"Join", "[", 
       RowBox[{"cosCoeff", ",", "sinCoeff"}], "]"}]}], ";", "\n", "\n", 
     RowBox[{"totalRadius", "=", 
      RowBox[{
       RowBox[{"Total", "[", 
        RowBox[{"Abs", "@", "cosCoeff"}], "]"}], "+", 
       RowBox[{"Total", "[", 
        RowBox[{"Abs", "@", "sinCoeff"}], "]"}]}]}], ";", "\n", "\t", 
     RowBox[{"basicCosCoord", "=", 
      RowBox[{"MapThread", "[", 
       RowBox[{"Times", ",", 
        RowBox[{"{", 
         RowBox[{"cosCoeff", ",", 
          RowBox[{"Cos", "[", 
           RowBox[{
            RowBox[{"Range", "[", "numCir", "]"}], "*", "$time"}], "]"}]}], 
         "}"}]}], "]"}]}], ";", "\n", "\t", 
     RowBox[{"basicSinCoord", "=", 
      RowBox[{"MapThread", "[", 
       RowBox[{"Times", ",", 
        RowBox[{"{", 
         RowBox[{"cosCoeff", ",", 
          RowBox[{"Sin", "[", 
           RowBox[{
            RowBox[{"Range", "[", "numCir", "]"}], "*", "$time"}], "]"}]}], 
         "}"}]}], "]"}]}], ";", "\n", "\t", 
     RowBox[{"manipulatedCosCoord", "=", 
      RowBox[{"Join", "[", "\n", "\t\t", 
       RowBox[{
        RowBox[{"Take", "[", "\n", "\t\t\t", 
         RowBox[{
          RowBox[{"Transpose", "[", "\n", "\t\t\t", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"basicCosCoord", "-", "totalRadius", "-", "5"}], ")"}],
               "/.", 
              RowBox[{"Cos", "\[Rule]", "Sin"}]}], ",", "basicCosCoord"}], 
            "}"}], "]"}], ",", "1"}], "]"}], ",", "\n", "\t\t", 
        RowBox[{"Drop", "[", 
         RowBox[{
          RowBox[{"Transpose", "[", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"basicCosCoord", "/.", 
              RowBox[{"Cos", "\[Rule]", "Sin"}]}], ",", "basicCosCoord"}], 
            "}"}], "]"}], ",", "1"}], "]"}]}], "]"}]}], ";", "\n", "\t", 
     RowBox[{"manipulatedSinCoord", "=", 
      RowBox[{"Join", "[", "\n", "\t\t", 
       RowBox[{
        RowBox[{"Take", "[", "\n", "\t\t\t", 
         RowBox[{
          RowBox[{"Transpose", "[", "\n", "\t\t\t", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"basicSinCoord", "-", "totalRadius", "-", "5"}], ")"}],
               "/.", 
              RowBox[{"Sin", "\[Rule]", "Cos"}]}], ",", "basicSinCoord"}], 
            "}"}], "]"}], ",", "1"}], "]"}], ",", "\n", "\t\t", 
        RowBox[{"Drop", "[", 
         RowBox[{
